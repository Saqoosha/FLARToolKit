package org.libspark.flartoolkit.core.rasterreader {	import org.libspark.flartoolkit.core.raster.rgb.FLARRgbRaster_BitmapData;	import org.libspark.flartoolkit.core.rasterreader.IFLARRgbPixelReader;		import flash.display.BitmapData;		/**	 * @author Saqoosha	 */	public class FLARBitmapDataReader implements IFLARRgbPixelReader {				private var _parent:FLARRgbRaster_BitmapData;		public function FLARBitmapDataReader(parent:FLARRgbRaster_BitmapData) {			this._parent = parent;		}		public function getPixel(i_x:int, i_y:int, o_rgb:Array):void {			var c:int = this._parent.bitmapData.getPixel(i_x, i_y);			o_rgb[0] = (c >> 16) & 0xff;// R			o_rgb[1] = (c >> 8) & 0xff;// G			o_rgb[2] = c & 0xff;// B			return;		}				public function getPixelSet(i_x:Array, i_y:Array, i_num:int, o_rgb:Array):void {			var bmp:BitmapData = this._parent.bitmapData;			for (var i:int = 0; i < i_num; i++) {				var c:int = bmp.getPixel(i_x[i], i_y[i]);				o_rgb[i*3+0] = (c >> 16) & 0xff;			    o_rgb[i*3+1] = (c >> 8) & 0xff;			    o_rgb[i*3+2] = c & 0xff;			}		}	}}